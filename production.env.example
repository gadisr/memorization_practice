# Production Environment Variables
# Copy this file to production.env and fill in your actual values

# Database Configuration (Managed PostgreSQL)
DATABASE_URL=postgresql://username:password@your-managed-db-host:5432/your-database-name

# Firebase Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_CREDENTIALS_PATH=/app/firebase-credentials.json

# CORS Origins (Production domains)
ALLOWED_ORIGINS=["https://blindfoldcubing.com", "https://www.blindfoldcubing.com"]

# Frontend API URL
API_BASE_URL=https://blindfoldcubing.com/api/v1
